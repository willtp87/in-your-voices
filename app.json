{
  "expo": {
    "name": "In Your Voices",
    "slug": "in-your-voices",
    "scheme": "voices-scheme",
    "platforms": [
      "ios"
    ],
    "newArchEnabled": true,
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIFileSharingEnabled": true,
        "ITSAppUsesNonExemptEncryption": false,
        "LSSupportsOpeningDocumentsInPlace": true
      },
      "entitlements": {
        "aps-environment": "development"
      },
      "bundleIdentifier": "com.willtp87.inyourvoices"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker",
      "expo-localization"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4561795f-de9b-48c3-b69b-4d45f642a67a"
      }
    }
  }
}
